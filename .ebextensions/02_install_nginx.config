container_commands:
  02_install_nginx:
    command: sudo yum install nginx -y
03_install_php-fpm:
    command: rpm -Uvh https://dl.fedoraproject.org/pub/epel/epel-release-latest-6.noarch.rpm
    command: rpm -Uvh https://mirror.webtatic.com/yum/el6/latest.rpm
    command: yum install -y php56w-fpm php56w-mbstring php56w-mysql php56w-pdo php56w-mcrypt php56w-cli php56w-pecl-apcu php56w-soap php56w-pear php56w-xml php56w-xmlrpc
